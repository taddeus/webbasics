<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
	<title>WebBasics documentation</title>
	<parser>
		<default-package-name>WebBasics</default-package-name>
		<target>build/docs</target>
	</parser>
	<transformer>
		<target>build/docs</target>
	</transformer>
	<transformations>
		<template name="abstract" />
	</transformations>
	<files>
		<directory>.</directory>
		<ignore>tests/*</ignore>
		<ignore>build/*</ignore>
		<ignore>php-activerecord/*</ignore>
	</files>
</phpdoc>
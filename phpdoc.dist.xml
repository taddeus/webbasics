<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
	<title>Minimalistic documentation</title>
	<parser>
		<default-package-name>Minimalistic</default-package-name>
		<target>build/docs</target>
	</parser>
	<transformer>
		<target>build/docs</target>
	</transformer>
	<files>
		<directory>.</directory>
		<ignore>tests/*</ignore>
	</files>
</phpdoc>